{
  "name": "@tuel/utils",
  "version": "1.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@tuel/tokens": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "Essential utilities and helper functions for TUEL animation components. Type-safe utilities for DOM manipulation, timing, and animation calculations.",
  "keywords": [
    "react",
    "utilities",
    "helpers",
    "animation",
    "typescript",
    "dom",
    "timing",
    "calculations"
  ],
  "author": "Omer Akben <contact@tuel.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/omerakben/tuel.git",
    "directory": "packages/utils"
  },
  "homepage": "https://tuel.ai/docs/utils",
  "bugs": {
    "url": "https://github.com/omerakben/tuel/issues"
  }
}
